{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.0-beta.28.3",
    "name": "mytb"
  },
  "apps": [
    {
      "name": "frontend",
      "platform": "browser",
      "root": "src/",
      "outDir": "dist/",
      "assets": [
      ],
      "index": "nonExistingDummy.html",
      "main": "nonExistingDummy.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.app.json",
      "testTsconfig": "../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
      ],
      "scripts": [],
      "serviceWorker": "false"
    }
  ],
  "test": {
    "karma": {
      "config": "karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "build": {
      "preserveSymlinks": true
    },
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
