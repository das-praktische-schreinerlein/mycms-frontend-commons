{
  "name": "@dps/mycms-frontend-commons",
  "version": "5.0.0",
  "license": "Apache-2.0",
  "angular-cli": {},
  "scripts": {
    "build": "npm run prepare-test && npm run prepare-build && tsc -p tsconfig.json && npm run build-copy && npm run build-prune",
    "build-copy": "cpx \"src/**/*.{html,css,ts}\" dist",
    "build-prune": "del-cli \"dist/testing/**/*.ts\" \"!dist/testing/**/*.d.ts\" && del-cli \"dist/angular-commons/testing/**/*.ts\" \"!dist/angular-commons/testing/**/*.d.ts\" &&  del-cli \"dist/**/cdoc-routing.service.ts\" && del-cli \"dist/**/common-routing.service.ts\" && del-cli \"dist/test.ts\" && del-cli \"dist/polyfills.ts\" && del-cli \"dist/main.spec.ts\"",
    "prepare": "patch-package",
    "prepare-build": "cross-os prepare-build-os",
    "prepare-build-os": {
      "darwin": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
      "linux": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
      "win32": "node_modules\\.bin\\rimraf dist && node_modules\\.bin\\mkdirp dist"
    },
    "serve-mycms-frontend-devtools": "live-server --port=4201 --no-browser --host=localhost --entry-file=index.html dist/devtools/httproot",
    "prepare-test": "cross-os prepare-test-os",
    "prepare-test-os": {
      "darwin": "../node_modules/.bin/rimraf tsc-out",
      "linux": "./node_modules/.bin/rimraf tsc-out",
      "win32": "node_modules\\.bin\\rimraf tsc-out"
    },
    "test": "ng test --singleRun=true --log-level=info --app=frontend --preserve-symlinks=true --environment=dev",
    "testCoverage": "ng test --singleRun=true --log-level=info --app=frontend --preserve-symlinks=true --environment=dev --code-coverage true",
    "testwatch": "ng test --log-level=info --app=frontend --preserve-symlinks=true --environment=dev"
  },
  "main": "dist/",
  "types": "dist/",
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.2",
    "@angular/common": "5.0.2",
    "@angular/compiler": "5.0.2",
    "@angular/core": "5.0.2",
    "@angular/forms": "5.0.2",
    "@angular/http": "5.0.2",
    "@angular/platform-browser": "5.0.2",
    "@angular/platform-browser-dynamic": "5.0.2",
    "@angular/platform-server": "5.0.2",
    "@angular/router": "5.0.2",
    "@angular/service-worker": "1.0.0-beta.16",
    "@dps/mycms-commons": "git://github.com/das-praktische-schreinerlein/mycms-commons.git#5.0.0",
    "@ng-bootstrap/ng-bootstrap": "~1.1.1",
    "@ngx-translate/core": "~8.0.0",
    "@ngx-translate/http-loader": "~2.0.1",
    "angular-2-dropdown-multiselect": "~1.8.1",
    "angular2-cookie-law": "~1.4.0",
    "angular2-markdown": "~2.2.3",
    "angulartics2": "~5.4.0",
    "bootstrap": "~4.1.3",
    "deep-equal": "~1.0.1",
    "detect-browser": "~3.0.0",
    "jquery": "~3.3.1",
    "js-data": "3.0.3",
    "leaflet": "1.6.0",
    "leaflet-editable-polyline": "1.0.8",
    "leaflet.markercluster": "1.4.1",
    "ngx-file-drop": "3.0.2",
    "ngx-lightbox": "~1.1.0",
    "ngx-toastr": "~8.10.2",
    "promise-serial": "~0.1.5",
    "rxjs": "~5.5.2",
    "tz-lookup": "~6.1.24",
    "vis": "4.21.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "~0.0.20",
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "~5.2.10",
    "@compodoc/compodoc": "1.1.11",
    "@types/fluent-ffmpeg": "2.1.14",
    "@types/jasmine": "3.5.10",
    "@types/leaflet": "1.5.12",
    "@types/leaflet.markercluster": "1.4.2",
    "@types/node": "13.11.1",
    "@types/vis": "4.21.19",
    "babel-core": "6.26.3",
    "babel-loader": "8.1.0",
    "codelyzer": "5.2.2",
    "cpx": "1.5.0",
    "cross-os": "1.3.0",
    "del-cli": "3.0.0",
    "enhanced-resolve": "4.1.1",
    "istanbul": "0.4.5",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "5.0.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "karma-phantomjs-launcher": "1.0.4",
    "mkdirp": "1.0.4",
    "patch-package": "6.2.2",
    "postcss-load-config": "2.1.0",
    "postcss-loader": "3.0.0",
    "protractor": "5.4.3",
    "rimraf": "3.0.2",
    "string-replace-loader": "2.3.0",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  }
}
