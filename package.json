{
  "name": "@dps/mycms-frontend-commons",
  "version": "4.2.0",
  "license": "Apache-2.0",
  "angular-cli": {},
  "scripts": {
    "build": "npm run prepare-build && tsc && npm run build-copy && npm run build-prune",
    "build-copy": "cpx \"src/**/*.{html,css,ts}\" dist",
    "build-prune": "del-cli \"dist/testing/**/*.ts\" \"!dist/testing/**/*.d.ts\" && del-cli \"dist/angular-commons/testing/**/*.ts\" \"!dist/angular-commons/testing/**/*.d.ts\" &&  del-cli \"dist/**/cdoc-routing.service.ts\" && del-cli \"dist/**/common-routing.service.ts\"",
    "prepare": "patch-package",
    "prepare-build": "cross-os prepare-build-os",
    "prepare-build-os": {
      "darwin": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
      "linux": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
      "win32": "node_modules\\.bin\\rimraf dist && node_modules\\.bin\\mkdirp dist"
    },
    "serve-mycms-frontend-devtools": "live-server --port=4201 --no-browser --host=localhost --entry-file=index.html dist/devtools/httproot",
    "test": "echo 'no tests'"
  },
  "main": "dist/",
  "types": "dist/",
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.2",
    "@angular/common": "5.0.2",
    "@angular/compiler": "5.0.2",
    "@angular/core": "5.0.2",
    "@angular/forms": "5.0.2",
    "@angular/http": "5.0.2",
    "@angular/platform-browser": "5.0.2",
    "@angular/platform-browser-dynamic": "5.0.2",
    "@angular/platform-server": "5.0.2",
    "@angular/router": "5.0.2",
    "@angular/service-worker": "1.0.0-beta.16",
    "@dps/mycms-commons": "git://github.com/das-praktische-schreinerlein/mycms-commons.git#4.2.0",
    "@ng-bootstrap/ng-bootstrap": "~1.1.1",
    "@ngx-translate/core": "~8.0.0",
    "@ngx-translate/http-loader": "~2.0.1",
    "angular-2-dropdown-multiselect": "~1.8.1",
    "angular2-cookie-law": "~1.4.0",
    "angular2-markdown": "~2.2.3",
    "angulartics2": "~5.4.0",
    "bootstrap": "~4.1.3",
    "deep-equal": "~1.0.1",
    "detect-browser": "~3.0.0",
    "jquery": "~3.3.1",
    "js-data": "~3.0.3",
    "leaflet": "~1.3.4",
    "leaflet.markercluster": "~1.4.0",
    "ngx-lightbox": "~1.1.0",
    "ngx-toastr": "~8.10.2",
    "ngx-file-drop": "~3.0.2",
    "promise-serial": "~0.1.5",
    "rxjs": "~5.5.2",
    "vis": "~4.21.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "~0.0.20",
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "~5.2.10",
    "@compodoc/compodoc": "~1.1.5",
    "@types/fluent-ffmpeg": "~2.1.6",
    "@types/jasmine": "2.8.2",
    "@types/leaflet": "~1.2.10",
    "@types/leaflet.markercluster": "~1.0.3",
    "@types/node": "~9.6.2",
    "@types/vis": "~4.21.7",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.4",
    "codelyzer": "~4.4.4",
    "cpx": "~1.5.0",
    "cross-os": "~1.3.0",
    "del-cli": "~1.1.0",
    "enhanced-resolve": "~4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-phantomjs-launcher": "~1.0.4",
    "mkdirp": "~0.5.1",
    "patch-package": "~5.1.1",
    "postcss-load-config": "~1.2.0",
    "postcss-loader": "~2.1.4",
    "protractor": "~5.4.0",
    "rimraf": "~2.6.2",
    "string-replace-loader": "~2.1.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.8.3"
  }
